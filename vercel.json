{
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/privacy(?:\\.html)?$", "dest": "/privacy.html" },
    { "src": "^/terms(?:\\.html)?$", "dest": "/terms.html" },
    { "src": "^/(tiktok.*\\.txt)$", "dest": "/$1" },
    { "src": "^/(.*\\..*)$", "dest": "/$1" },
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}
